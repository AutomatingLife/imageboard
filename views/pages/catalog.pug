extends ../layout.pug
include ../mixins/catalogtile.pug

block head
	title /#{board._id}/ - Catalog

block content
	include ../includes/boardheader.pug
	hr(size=1)
	.mode Catalog View [#[a.no-decoration(href=`/${board._id}`) Go Back]]
	hr(size=1)
	if threads.length === 0
		p No posts.
		hr(size=1)
	section.catalog
		for thread in threads
			+catalogtile(board, thread)

